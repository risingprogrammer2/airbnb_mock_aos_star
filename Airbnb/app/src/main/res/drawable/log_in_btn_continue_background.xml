<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <!-- 테두리 내부 색상 설정 -->
            <solid
                android:color="@color/greyForLogInBtnContinue"/>

            <!-- 테두리 내부 여백 설정 -->
            <padding/>

            <!-- 테두리 설정: 두께, 색상 -->
            <stroke
                android:width="1dp"
                android:color="@color/greyForLogInBtnContinue" />

            <corners android:radius="8dp"/>
        </shape>
    </item>
</layer-list>
